<?xml version="1.0" encoding="windows-1252" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
             version="1.0">
  <persistence-unit name="Model">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>java:/app/jdbc/jdbc/TMSAPPCONDS</jta-data-source>
    <class>training.iqgateway.OffenceDetails</class>
    <class>training.iqgateway.OffenceTypes</class>
    <class>training.iqgateway.Owners</class>
    <class>training.iqgateway.Registrations</class>
    <class>training.iqgateway.Roles</class>
    <class>training.iqgateway.Users</class>
    <class>training.iqgateway.Vehicles</class>
    <properties>
      <property name="eclipselink.target-server" value="WebLogic_10"/>
      <property name="javax.persistence.jtaDataSource"
                value="java:/app/jdbc/jdbc/TMSAPPCONDS"/>
    </properties>
  </persistence-unit>
</persistence>
